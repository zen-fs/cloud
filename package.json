		"eslint": "^9.11.1",
		"prettier": "^3.2.5",
		"tsx": "^4.19.1",
		"typedoc": "^0.27.6",
		"typescript": "^5.7.3",
		"typescript-eslint": "^8.8.0"
	},
	"peerDependencies": {
		"@zenfs/core": "^2.0.0",
		"utilium": "^1.9.0"
	},
	"optionalDependencies": {
		"@aws-sdk/client-s3": "^3.679.0",
