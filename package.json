		"Marcos Candeia <marrcooos@gmail.com>"
	],
	"publishConfig": {
		"access": "public",
		"provenance": true
	},
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/zen-fs/cloud.git"
	},
	"bugs": {
		"url": "https://github.com/zen-fs/cloud/issues"
	},
	"engines": {
		"node": ">= 18"
	},
	"files": [
		"dist",
		"src",
		"license.md",
		"tsconfig.json"
	],
	"exports": {
		".": "./dist/index.js",
		"./*": "./dist/*"
	},
	"scripts": {
		"format": "prettier --write .",
		"format:check": "prettier --check .",
