{
	"name": "@zenfs/cloud",
	"version": "0.1.3",
	"description": "ZenFS Cloud backends",
	"funding": {
		"type": "individual",
		"url": "https://github.com/sponsors/james-pre"
	},
	"main": "dist/index.js",
	"types": "src/index.ts",
	"keywords": [
		"filesystem",
		"dropbox",
		"S3"
	],
	"type": "module",
	"homepage": "https://github.com/zen-fs/cloud",
	"author": "James Prevett <jp@jamespre.dev> (https://jamespre.dev)",
